@import '/towxml/style/main.wxss';
@import '/towxml/style/theme/light.wxss';
@import '/towxml/style/theme/dark.wxss';
@import '/utils/icon.wxss';
.template{
  padding-bottom: 40px;
}
.options{
  background-color: white;
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: space-around;
  height: 40px;
  line-height: 40px;
  z-index: 998
}
.listcontent{
  width: 75%;
  position: fixed;
  left: -75%;
  top: 0;
  bottom: 80rpx;
  background: #fff;
  z-index: 600;
  overflow: auto
}
.title-one{
  line-height: 40rpx;
  color:#252525;
  font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
}
.title-one text{
  line-height: 40rpx;
  height: 40rpx;
  font-size: 28rpx;
  margin: 0 20px;
  display: block;
  border-bottom: 2rpx solid #f1f1f1;
}