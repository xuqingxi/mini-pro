/* pages/library/library.wxss */
.iconlist image{
  width: 50px;
  height: 50px;
  border: 1px solid silver;
  border-radius: 50%;
  box-sizing: border-box;
}
.img-wrap{
  display:flex;
flex-wrap:wrap;
justify-content:space-between;
padding:0 10px;

}
.item-wrap{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 20px
}
.animating{
  animation: ani .5s ease
}
.iconlist{
  display: inline-block;
}
@keyframes ani{
  0%   {transform: scale(1)}
50%  {transform: scale(1.5)}
100% {transform: scale(1)}
}
.itemBook{
  width: 100px;
  text-align: center;
  margin-bottom: 20px;
  font-size: 10px
}
.item-wrap::after{
  content: '';
  width: 100px;
  display: inline-block
}
.itemBook image{
  width: 80px;
  height: 80px
}


